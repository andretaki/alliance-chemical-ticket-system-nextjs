# Alliance Chemical Ticket System - Local Development Environment
# Copy this file to .env.local and fill in your values

# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/alliance_tickets_dev"

# NextAuth Configuration
NEXTAUTH_URL="http://localhost:3001"
NEXTAUTH_SECRET="your-local-secret-key-here"

# Microsoft Graph Integration
MICROSOFT_GRAPH_CLIENT_ID="your-client-id"
MICROSOFT_GRAPH_CLIENT_SECRET="your-client-secret"
MICROSOFT_GRAPH_TENANT_ID="your-tenant-id"
MICROSOFT_GRAPH_WEBHOOK_SECRET="your-webhook-secret"

# Shopify Integration
SHOPIFY_STORE_URL="your-store.myshopify.com"
SHOPIFY_ADMIN_ACCESS_TOKEN="your-admin-token"
SHOPIFY_API_KEY="your-api-key"
SHOPIFY_API_SECRET="your-api-secret"
SHOPIFY_AUTO_CREATE_CUSTOMERS="true"

# AI Services
OPENAI_API_KEY="your-openai-key"
GROQ_API_KEY="your-groq-key"
GOOGLE_API_KEY="your-google-key"

# Email Services
MAILGUN_DOMAIN="your-domain.com"
MAIL_API_KEY="your-mailgun-key"

# KV Storage (Redis)
KV_URL="your-redis-url"
KV_REST_API_URL="your-kv-rest-url"
KV_REST_API_TOKEN="your-kv-token"
KV_REST_API_READ_ONLY_TOKEN="your-kv-readonly-token"

# Security & Secrets
CRON_SECRET="your-cron-secret"
EMAIL_PROCESSING_SECRET="your-email-processing-secret"

# Development Settings
DEBUG_SQL="true"
NODE_ENV="development"
FORM_URL="localhost:3001"
INTERNAL_DOMAIN="localhost"

# Testing Configuration
TEST_DATABASE_URL="postgresql://username:password@localhost:5432/alliance_tickets_test"